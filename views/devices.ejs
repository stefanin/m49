                <td scope="row"><%=d.RebootTask%></td>
                <td scope="row"><%=d.InstallEnv%></td>
                <td scope="row"><%=d.TerminalId%></td>
                <td scope="row"><%=d.IsPosLead%></td>
                <td scope="row"><%=d.IsXServices%></td>
                <td scope="row"><%=d.EnvDbUrl%></td>
                <td scope="row"><%=d.EnvDbHost%></td>
                <td scope="row"><%=d.XstoreConnectionUrl%></td>
                <td scope="row"><%=d.XstoreConnectionHost%></td>
                <td scope="row"><%=d.IsMonitored%></td>
                <td scope="row"><%=d.IsSccmClient%></td>
