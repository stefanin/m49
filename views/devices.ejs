
<script>
      $(document).ready(function(){
      $("#<%=idSession%>Input").on("keyup", function() {
        var value = $(this).val().toLowerCase();
        $("#<%=idSession%>Table tr").filter(function() {
          $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
        });
      });
    });
</script>

<span class="badge badge-primary"><%=contaRiga%></span> &ensp;&ensp;<input id="<%=idSession%>Input" type="text" class="mr-sm-2" placeholder="search in the table ..">
<table  class="table table-sm">
    <thead>
        <td scope="col">Status</td>
        <td scope="col">Name</td>
        <td scope="col">User</td>
        <td scope="col">IP</td>
        <td scope="coll">XStoreVersion</td>
        <td scope="coll">XEnvVersion</td>
        <td scope="col">Uptime</td>
        <td scope="coll">CountryId</td>
        <td scope="coll">CurrencyId</td>
        <td scope="coll">RegionId</td>
